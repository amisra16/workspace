package com.practice.random;

public class IntegerAutoBoxing {

	public static void main(String[] args) {
		IntegerAutoBoxing integerAutoBoxing = new IntegerAutoBoxing();
		Integer x = 30, y = 30;
		integerAutoBoxing.matchValues(x,y);
		Integer a = 400, b = 400;
		integerAutoBoxing.matchValues(a, b);
		Integer m = new Integer(40);
		Integer n = new Integer(40);
		integerAutoBoxing.matchValues(m, n);

	}
	
	private void matchValues(Integer x, Integer y) {
		if (x == y)
			System.out.println("X "+x.hashCode()+ " is equal to Y " + y.hashCode());
		else
			System.out.println("X "+x.hashCode()+ " is not equal to Y " + y.hashCode());
	}

}
