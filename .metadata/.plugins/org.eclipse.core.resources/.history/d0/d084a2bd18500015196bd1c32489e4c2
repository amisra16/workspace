package com.practice.threadexample;

public class ThreadPratice implements Runnable {

	private int i;

	public static void main(String[] args) {
		ThreadPratice t = new ThreadPratice();
		for (int x = 0; x < 10; x++) {
			new Thread(t).start();
		}


	}

	@Override
	public void run() {
		if (i % 5 != 0) {
			i++;
		}
		for (int x = 0; x < 5; x++, i++) {
//			if (x > 1)
//				Thread.yield();
			System.out.println(i + "");
		}
		System.out.println(i + "");
	}

}
